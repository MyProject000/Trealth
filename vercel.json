{
    "builds": [
      {
        "src": "medbooker_project/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.10.0" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "medbooker_project/wsgi.py"
      }
    ]
  }
  